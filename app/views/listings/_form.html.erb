<%= form_for [@user, @listing] do |l| %>
  <%= render "shared/errors", object: @listing %>

   <div class="field">
     <%= l.label :item_name %><br>
     <%= l.text_field :item_name %>
   </div>
   <div class="field">
     <%= l.label :quantity %><br>
     <%= l.text_field :quantity %>
   </div>
   <div class="field">
     <%= l.label :perishable %><br>
     <%= l.text_field :perishable %>
   </div>
   <div class="field">
     <%= l.label :expiry_date %><br>
     <%= l.date_select :expiry_date %>
   </div>
   <div class="field">
     <%= l.label :available_until %><br>
     <%= l.date_select :available_until %>
   </div>
   <div class="button">
     <%= l.submit %>
   </div>

<% end %>
