<%= form_tag sessions_path, :method => :post do %>
<p class="field">
  <%= label_tag :email %> <br />
  <%= text_field_tag :email %>
</p>

<p class="field">
  <%= label_tag :password %> <br />
  <%= password_field_tag :password %>
</p>

<p class="field">
  <%= submit_tag "Log In" %>
</p>


<% end %>
